/* pages/tabbar/recipes/recipes.wxss */
page {
  background: white;
  height: 100%;
  border: 0px;
  display: flex;
  justify-content: center;
}

.scan-center{
  width: 100%;
  height: 30%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.scan-center image {
  border: 5rpx solid #78C613;
  border-radius: 5%;
  width: 300rpx;
}

.scan-text {
  text-align: center;
  font-size: 35rpx;
}